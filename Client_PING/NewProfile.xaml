<Window x:Class="Client_PING.NewProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client_PING"
        mc:Ignorable="d"
        Title="New Profile" Height="107.333" Width="400" KeyUp="Window_KeyUp" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Profile name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="14"/>
        <TextBox x:Name="TextBoxProfileName" HorizontalAlignment="Left" Height="23" Margin="106,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="270" FontSize="14"/>
        <Button x:Name="ButtonSaveProfile" Content="Save" HorizontalAlignment="Left" Margin="302,42,0,0" VerticalAlignment="Top" Width="74" RenderTransformOrigin="0.669,-0.208" FontSize="14" Click="ButtonSaveProfile_Click"/>

    </Grid>
</Window>
